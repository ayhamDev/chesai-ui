{
  "name": "chesai-ui",
  "private": false,
  "version": "3.0.1",
  "packageManager": "pnpm@10.12.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayhamdev/chesai-ui"
  },
  "main": "./dist/chesai-ui.umd.js",
  "module": "./dist/chesai-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chesai-ui.es.js",
      "require": "./dist/chesai-ui.umd.js"
    },
    "./styles.css": "./dist/chesai-ui.css"
  },

  "scripts": {
    "dev": "storybook dev -p 8000",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "npx @biomejs/biome lint --write ./src",
    "format": "npx @biomejs/biome check --write --organize-imports-enabled=true ./src",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "gen:context": "bun run ./scripts/context.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@floating-ui/react": "^0.27.16",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-one-time-password-field": "^0.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "focus-trap-react": "^11.0.4",
    "framer-motion": "^12.23.13",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.16",
    "react": "19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.0",
    "use-ripple-hook": "^1.0.24",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@biomejs/biome": "2.0.4",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-links": "9.0.12",
    "@storybook/addon-themes": "9.0.12",
    "@storybook/react-vite": "9.0.12",
    "@storybook/test": "8.6.14",
    "@tailwindcss/postcss": "4.1.10",
    "@tailwindcss/vite": "4.1.10",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "babel-loader": "10.0.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "json": "11.0.0",
    "lint-staged": "16.1.2",
    "postcss": "8.5.6",
    "prop-types": "15.8.1",
    "storybook": "9.0.12",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --write --no-errors-on-unmatched",
      "biome lint --write --no-errors-on-unmatched --files-ignore-unknown=true",
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
